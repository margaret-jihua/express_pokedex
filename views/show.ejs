<div class="container">
  <div class="row">
    <div class="col s12 m4">
      <div class="card">
        <div class="card-image">
          <img src="<%= myPoke.sprites.other['official-artwork'].front_default %> " >
        </div>
        <div class="card-content">
        <span class="card-title"><%= myPoke.name.toUpperCase() %></span>
          <p>Type: <%= myPoke.types[0].type.name %> </p>
          <p>Weight: <%= myPoke.weight %> </p>
          <p>Number of Moves: <%= myPoke.moves.length %> </p>
        </div>
      </div>
    </div>
  </div>
</div>

            



  